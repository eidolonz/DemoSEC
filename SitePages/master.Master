        table {
            font-family: 'Kanit';
            font-size: 0.8em;
        }

        .carousel-inner img {
            min-height: 200px;
            width: 100%;
        }

        .bullhorn {
            position: absolute;
            right: 12.7%;
            width: 4%;
            height: auto;
            z-index: 1;
            padding: 15px 5px;
            margin-top: 50px;
            font-family: 'Kanit';
            font-weight: 300;
            line-height: 0;
            color: white;
            background: -webkit-linear-gradient(#00a19a,#093b66);
            background: linear-gradient(#00a19a,#093b66);
            border-bottom-left-radius: 10px;
            border-top-left-radius: 10px;
            -webkit-transition: width 0.4s ease-in-out;
            transition: width 0.4s ease-in-out;
            animation-duration: 1.2s;
            animation-name: slidein;
            animation-iteration-count: infinite;
            animation-delay:1s;
            animation-direction: alternate;
        }

            .bullhorn span {
                font-size: 0em;
                line-height: 1;
                -webkit-transition: font-size 0.4s ease;
                transition: font-size 0.4s ease;
                animation-duration: 1.2s;
                animation-name: slidein-span;
                animation-iteration-count: infinite;
            animation-delay:1s;
                animation-direction: alternate;
            }

            .bullhorn img {
                width: 50px;
                float: left;
                padding: 0 10px;
                animation-duration: 1.2s;
                animation-name: slidein-img;
                animation-iteration-count: infinite;
            animation-delay:1s;
                animation-direction: alternate;
            }

            .bullhorn:hover {
                opacity: 1;
                width: 12%;
                font-size: 1em;
                padding: 15px;
                color: white;
                animation: none;
            }

                .bullhorn:hover span {
                    font-size: 1em;
                    -webkit-transition-delay: 0.1s; /* Safari */
                    transition-delay: 0.1s;
                    animation: none;
                }

                .bullhorn:hover img {
                    min-width: 50px;
                    width: 50%;
                    animation: none;
                }


        @keyframes slidein {
            from {
                width: 4%;
                padding: 15px 5px;
            }

            to {
                width: 12%;
                padding: 15px;
            }
        }

        @keyframes slidein-img {
            from {
                width: 50px;
            }

            to {
                min-width: 50px;
                width: 50%;
            }
        }
        @keyframes slidein-span {
            from {
                font-size: 0em;
                line-height: 1;
            }

            to {
                font-size: 1em;
                line-height: 1;
                color:white;
            }
        }



        .bg-banner-1 {
            background-image: url(../PublishingImages/slide_1.png);
            background-repeat: no-repeat;
            background-size: cover;
            width: 100%;
            min-height: 300px;
        }

        .bg-banner-2 {
            background-image: url(../PublishingImages/slide_2.png);
            background-repeat: no-repeat;
            background-size: cover;
