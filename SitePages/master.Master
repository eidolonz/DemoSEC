            <!-- JS for Custom Masterpage -->
            <script type="text/javascript">
                $(document).ready(function () {
                    fn_fix_breadcrumb_top_init();



                    $("body").css("display", "none");
                    $("body").fadeIn(1000);
                    $("a.transition").click(function (event) {
                        event.preventDefault();
                        linkLocation = this.href;
                        $("body").fadeOut(500, redirectPage);
                    });

                    function redirectPage() {
                        window.location = linkLocation;
                    }
                });

                function fn_fix_breadcrumb_top_init() {
                    fn_set_fix_breadcrumb_top();

                    $(window).scroll(function () {

                        fn_set_fix_breadcrumb_top();
                    });

                    function fn_set_fix_breadcrumb_top() {
                        var sWidth = 115;
                        if (window.matchMedia('(min-width: 768px)').matches) {
                            sWidth = 425;
                        }


                        if ($(window).scrollTop() < sWidth) {
                            if ($('.siteNav').hasClass('fixedTop')) {
                                $('.siteNav').removeClass('fixedTop');
                            }
                        } else {
                            if (!$('.siteNav').hasClass('fixedTop')) {
                                $('.siteNav').addClass('fixedTop');
                            }
                        }
                    }
                }


                function btnColor(btn) {
                    var property = document.getElementById(btn);
                    if ($('.menu-button').hasClass('toggled')) {
                        $('.menu-button').removeClass('toggled');
                    } else {
                        $('.menu-button').addClass('toggled');
                    }
                }
